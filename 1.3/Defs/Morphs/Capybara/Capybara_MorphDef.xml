<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="SensitiveFoodMorph">
		<defName>CapybaraMorph</defName>
		<label>capybaramorph</label>
		<description>Capybaramorphs are the result of a human exposed to high levels of mutating mechanites. They are highly sociable and stable due to their relaxed nature.</description>
		<race>Capybara</race>
		<classification>Rodent</classification>
		<categories>
			<li>Tropical</li>
			<li>Rodent</li>
		</categories>
		<injectorProperties></injectorProperties>
		<fullTransformation>PawnmorphCapybaraTF</fullTransformation>
		<partialTransformation>PawnmorphCapybaraTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(109, 84, 49)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>CapybaraTransformationMemory</transformationMemory>
			<revertedMemory>CapybaraRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>
